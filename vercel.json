{
    "builds": [
        {
            "src": "ecommerce/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "Python3.13.6"
            }
        }
    ],
    "routes": [
        {"src": "/(.*)", "dest": "ecommerce/wsgi.py"}
    ]
}